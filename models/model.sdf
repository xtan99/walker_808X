          <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
            <ros>
              <namespace>cam</namespace>

              <!-- topics need to be prefixed with camera_name for remapping -->
              <remapping>my_robot_cam/image_raw:=my_robot_cam/image_demo</remapping>
              <remapping>my_robot_cam/depth/image_raw:=my_robot_cam/depth_demo</remapping>
              <remapping>my_robot_cam/camera_info:=my_robot_cam/raw_cam_info_demo</remapping>
              <remapping>my_robot_cam/depth/camera_info:=my_robot_cam/depth_cam_info_demo</remapping>
              <remapping>my_robot_cam/points:=my_robot_cam/points_demo</remapping>
            </ros>

            <!-- omit camera_name to default to sensor name -->
            <camera_name>my_robot_cam</camera_name>

            <!-- frame_name>omit so it defaults to link name</frame_name-->

            <min_depth>0.1</min_depth>
            <max_depth>500</max_depth>
          </plugin>

          
